@model IEnumerable<WMS.Models.Invent.ReceivingLine>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_AdminlteNetcoreIndex.cshtml";
}


<table class="table table-hover table-striped">
    <thead>
        <tr>
                <th>
                    @Html.DisplayNameFor(model => model.QtyOrder)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.QtyReceive)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.QtyInventory)
                </th>
            <th>
                @Html.DisplayNameFor(model => model.Branch)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ItemType)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Receiving)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Warehouse)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.QtyOrder)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.QtyReceive)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.QtyInventory)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Branch.branchId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ItemType.ItemTypeId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Receiving.ReceivingId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Warehouse.warehouseId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ReceivingLineId"><i class="fa fa-pencil"></i></a> |
                <a asp-action="Details" asp-route-id="@item.ReceivingLineId"><i class="fa fa-list"></i></a> |
                <a asp-action="Delete" asp-route-id="@item.ReceivingLineId"><i class="fa fa-trash"></i></a>
            </td>
        </tr>
}
    </tbody>
</table>
