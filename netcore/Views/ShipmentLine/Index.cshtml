@model IEnumerable<WMS.Models.Invent.ShipmentLine>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_AdminlteNetcoreIndex.cshtml";
}


<table class="table table-hover table-striped">
    <thead>
        <tr>
                <th>
                    @Html.DisplayNameFor(model => model.Qty)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.QtyShipment)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.QtyInventory)
                </th>
            <th>
                @Html.DisplayNameFor(model => model.Branch)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ItemType)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Shipment)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Warehouse)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Qty)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.QtyShipment)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.QtyInventory)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Branch.BranchId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ItemType.ItemTypeId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Shipment.ShipmentId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Warehouse.warehouseId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ShipmentLineId"><i class="fa fa-pencil"></i></a> |
                <a asp-action="Details" asp-route-id="@item.ShipmentLineId"><i class="fa fa-list"></i></a> |
                <a asp-action="Delete" asp-route-id="@item.ShipmentLineId"><i class="fa fa-trash"></i></a>
            </td>
        </tr>
}
    </tbody>
</table>
